function clickListener(e){e.addEventListener?e.addEventListener("click",function(t){toggleInline(e),t.preventDefault()}):e.attachEvent("onclick",function(t){toggleInline(e),t.preventDefault()})}function indexOf(e,t){for(var i=0;i<e.length;i++)if(e[i]===t)return i;return-1}function toggleInline(e){var t=document.getElementById("d"+e.id.slice(1));t&&("none"==t.style.display?showInline("d"+e.id.slice(1)):hideInline("d"+e.id.slice(1)))}function showInline(e){var t=document.getElementById(e);t&&(t.style.display="inline",t.setAttribute("tabindex",-1),t.focus())}function hideInline(e){var t=document.getElementById(e);t&&(t.removeAttribute("tabindex"),t.style.display="none")}var links=document.querySelectorAll(".acronymtemplate");if(links.length>0){for(var footnote=document.createElement("div"),ol=document.createElement("ol"),definitionList=[],thisDefinition,num=1,definitionID,abbr,sup,i=0;i<links.length;i++){definitionID=links[i].id.slice(1),thisDefinition=document.getElementById("d"+definitionID).innerHTML,abbr=document.createElement("abbr"),links[i].setAttribute("href","#d"+definitionID.slice(1)),links[i].setAttribute("title",thisDefinition),abbr.setAttribute("title",thisDefinition),abbr.innerHTML=links[i].outerHTML,clickListener(abbr.firstChild),sup=document.createElement("sup"),sup.setAttribute("class","printOnly");var index=indexOf(definitionList,thisDefinition);if(-1==index){var li=document.createElement("li"),li_txt=document.createTextNode(thisDefinition);li.appendChild(li_txt),ol.appendChild(li),definitionList.push(thisDefinition),sup.innerHTML=num,num++}else sup.innerHTML=index+1;abbr.appendChild(sup),links[i].parentNode.replaceChild(abbr,links[i])}footnote.innerHTML="<h3>Footnotes</h3>",footnote.setAttribute("class","printOnly bodytext"),footnote.appendChild(ol),document.getElementById("start-panel").appendChild(footnote)}
